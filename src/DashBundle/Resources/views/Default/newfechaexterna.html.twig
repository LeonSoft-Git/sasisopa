        {{ form_start(form,  {'action':path('nfcd', {'id': id})}) }}
        <div class="row">
            <div class="col-lg-4">

            </div>
            <div class="col-lg-8">
                <label>Fecha última revisión</label>
                {{ form_widget(form.fecha, {'attr': {'class': 'form-control date'}} ) }}
